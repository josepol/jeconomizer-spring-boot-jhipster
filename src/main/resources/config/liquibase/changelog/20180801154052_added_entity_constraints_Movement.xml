<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Movement.
    -->
    <changeSet id="20180801154052-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="category_id_id"
                                 baseTableName="movement"
                                 constraintName="fk_movement_category_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="category"/>

        <addForeignKeyConstraint baseColumnNames="user_id_id"
                                 baseTableName="movement"
                                 constraintName="fk_movement_user_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
